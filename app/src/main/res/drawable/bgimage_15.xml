<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Set màu nền-->
    <solid
        android:color="#cd853f" >
    </solid>

    <!-- đặt thuộc tính màu và độ rộng của đường viền -->
    <stroke
        android:width="5dp"
        android:color="#cd853f">
    </stroke>

    <!-- các thuộc tính căn chỉnh-->
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp">
    </padding>

    <corners
        android:radius="500dp" >
    </corners>
</shape>